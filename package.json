{
  "name": "serverless-kms-alias",
  "version": "3.0.0",
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.338.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.194",
    "@types/serverless": "3.12.11",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "8.41.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "44.2.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "markdownlint-cli": "0.34.0",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-packagejson": "2.4.3",
    "typescript": "5.0.4"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint:markdown": "prettier --write '*.md' '!(node_modules|dist)/**/*.md' && markdownlint '*.md' '!(node_modules|dist)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "prepublishOnly": "rm -rf dist && npm run build && npm run lint && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.md": ["prettier --write", "markdownlint --config=.github/linters/.markdown-lint.yml --fix"],
    "./*.js": ["eslint --fix"],
    "*.ts": ["eslint --fix"]
  },
  "repository": "github:serverless-plugins/serverless-kms-alias"
}
