{
  "name": "serverless-kms-alias",
  "version": "4.0.1",
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.437.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.200",
    "@types/serverless": "3.12.17",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "markdownlint-cli": "0.37.0",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "3.0.3",
    "prettier-plugin-packagejson": "2.4.6",
    "typescript": "5.2.2"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint:markdown": "prettier --write '*.md' '!(node_modules|dist)/**/*.md' && markdownlint '*.md' '!(node_modules|dist)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "prepublishOnly": "rm -rf dist && npm run build && npm run lint && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.md": [
      "prettier --write",
      "markdownlint --config=.github/linters/.markdown-lint.yml --fix"
    ],
    "./*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "repository": "github:serverless-plugins/serverless-kms-alias"
}
