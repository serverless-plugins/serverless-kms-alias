{
  "name": "serverless-kms-alias",
  "version": "5.0.11",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "unbuild",
    "check:types": "tsc --noEmit",
    "lint:markdown": "prettier --write '*.md' '!(node_modules|dist)/**/*.md' && markdownlint '*.md' '!(node_modules|dist)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "prettier --write package.json && eslint --fix",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "prepublishOnly": "rm -rf dist && npm run build && npm run lint && pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.md": [
      "prettier --write",
      "markdownlint --config=.github/linters/.markdown-lint.yml --fix"
    ],
    "./*.cjs": [
      "eslint --fix"
    ],
    "./*.mjs": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "license": "MIT",
  "repository": "github:serverless-plugins/serverless-kms-alias",
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "3.968.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/npm": "13.1.3",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/lodash": "4.17.23",
    "@types/serverless": "3.12.27",
    "eslint": "9.39.2",
    "eslint-config-decent": "3.1.93",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "markdownlint-cli": "0.47.0",
    "npm-run-all2": "8.0.4",
    "pinst": "3.0.0",
    "prettier": "3.7.4",
    "prettier-plugin-packagejson": "2.5.21",
    "semantic-release": "25.0.2",
    "typescript": "5.9.3",
    "unbuild": "3.6.1"
  }
}
