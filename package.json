{
  "name": "serverless-kms-alias",
  "version": "5.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "unbuild",
    "check:types": "tsc --noEmit",
    "lint:markdown": "prettier --write '*.md' '!(node_modules|dist)/**/*.md' && markdownlint '*.md' '!(node_modules|dist)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "prettier --write package.json && eslint --fix",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "prepublishOnly": "rm -rf dist && npm run build && npm run lint && pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.md": ["prettier --write", "markdownlint --config=.github/linters/.markdown-lint.yml --fix"],
    "./*.cjs": ["eslint --fix"],
    "./*.mjs": ["eslint --fix"],
    "*.ts": ["eslint --fix"]
  },
  "license": "MIT",
  "repository": "github:serverless-plugins/serverless-kms-alias",
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.637.0"
  },
  "devDependencies": {
    "@types/lodash": "4.17.7",
    "@types/serverless": "3.12.22",
    "eslint": "9.9.1",
    "eslint-config-decent": "2.2.0",
    "husky": "9.1.5",
    "lint-staged": "15.2.9",
    "markdownlint-cli": "0.41.0",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.2",
    "typescript": "5.5.4",
    "unbuild": "2.0.0"
  }
}
